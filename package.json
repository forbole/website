    "jsdom": "^22.1.0",
    "markdown-to-jsx": "^7.1.9",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "multer": "^1.4.5-lts.1",
    "next": "^14.0.3",
    "next-sitemap": "^4.2.3",
    "next-translate": "^2.6.2",
    "next-translate-plugin": "^2.6.2",
    "nodemailer": "^6.7.5",
