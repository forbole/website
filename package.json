{
  "name": "forbole-website",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "analyze-bundle": "BUNDLE_ANALYZER=true yarn build",
    "build": "npm run build:next && npm run build:server",
    "build:next": "cross-env NODE_ENV=production next build && next-sitemap",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "next dev",
    "e2e": "playwright test",
    "i18next": "i18next 'src/**/*.{tsx,ts,jsx,js}'",
    "lint": "bash scripts/lint.sh",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:prettier": "prettier --check src/**/* server/**/* graphql/**/*",
    "lint:prettier:fix": "prettier --write src/**/* server/**/* graphql/**/*",
    "prd": "pm2 start ecosystem.config.js",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo no tests",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
