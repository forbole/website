{
  "presets": [
    [
      "next/babel"
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@modules": "./modules",
          "@components": "./components",
          "@layout": "./layout",
          "@actions": "./actions",
          "@actionTypes": "./actionTypes",
          "@static": "./static",
          "@store": "./store",
          "@sagas": "./sagas",
          "@reducers": "./reducers",
          "@dummy": "./dummy"
        }
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ],
        "next/babel"
      ]
    }
  }
}
